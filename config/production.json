{
  "environment": "production",
  "app": {
    "name": "Gig Grid",
    "version": "1.0.0",
    "debug": false
  },
  "api": {
    "baseUrl": "https://api.gigrid.com",
    "timeout": 15000,
    "mockData": false
  },
  "database": {
    "type": "dynamodb",
    "region": "ap-southeast-1",
    "tablePrefix": "GigGrid-PROD-",
    "billingMode": "PAY_PER_REQUEST",
    "freeTier": true,
    "readCapacityUnits": 5,
    "writeCapacityUnits": 5,
    "tables": {
      "users": "GigGrid-PROD-Users",
      "artists": "GigGrid-PROD-Artists",
      "events": "GigGrid-PROD-Events",
      "bookings": "GigGrid-PROD-Bookings",
      "payments": "GigGrid-PROD-Payments",
      "reviews": "GigGrid-PROD-Reviews"
    },
    "backupEnabled": false,
    "pointInTimeRecovery": false
  },
  "email": {
    "provider": "aws-ses",
    "region": "ap-southeast-1",
    "sandboxMode": true,
    "freeTier": true,
    "monthlyLimit": 200,
    "from": "noreply@gigrid.com",
    "replyTo": "support@gigrid.com",
    "dkim": false
  },
  "authentication": {
    "provider": "aws-cognito",
    "region": "ap-southeast-1",
    "freeTier": true,
    "monthlyActiveUsers": 50000,
    "userPoolId": "${AWS_COGNITO_USER_POOL_ID_PROD}",
    "clientId": "${AWS_COGNITO_CLIENT_ID_PROD}",
    "sessionTimeout": 7200000,
    "enableTwoFactor": false,
    "mfaConfiguration": "OFF"
  },
  "storage": {
    "provider": "aws-s3",
    "region": "ap-southeast-1",
    "bucket": "gigrid-prod-uploads",
    "freeTier": true,
    "storageLimit": "5GB",
    "maxFileSize": "5MB",
    "requestLimit": 20000,
    "allowedTypes": ["image/jpeg", "image/png", "image/webp", "audio/mpeg", "audio/wav"],
    "cdn": {
      "enabled": false
    },
    "versioning": false,
    "encryption": false
  },
  "aws": {
    "amplify": {
      "appId": "${AWS_AMPLIFY_APP_ID_PROD}",
      "branch": "main",
      "freeTier": true,
      "buildTimeLimit": 10
    },
    "lambda": {
      "region": "ap-southeast-1",
      "freeTier": true,
      "requestLimit": 1000000,
      "durationLimit": 400000,
      "memorySize": 128,
      "timeout": 15
    },
    "apiGateway": {
      "region": "ap-southeast-1",
      "stage": "prod",
      "freeTier": true,
      "requestLimit": 1000000,
      "caching": false
    }
  },
  "features": {
    "registration": true,
    "socialLogin": false,
    "emailVerification": true,
    "profileManagement": true,
    "artistVerification": true,
    "paymentProcessing": false,
    "bookingSystem": true,
    "messaging": false,
    "notifications": false,
    "analytics": false,
    "reporting": false
  },
  "ui": {
    "theme": "production",
    "showDebugInfo": false,
    "enableHotReload": false
  },
  "logging": {
    "level": "error",
    "cloudWatch": true,
    "logGroup": "/aws/lambda/gigrid-prod",
    "console": false,
    "retention": 7,
    "freeTier": true
  },
  "monitoring": {
    "enabled": false,
    "cloudWatch": true,
    "xray": false,
    "performanceMonitoring": false,
    "alerting": false,
    "freeTier": true
  },
  "security": {
    "rateLimit": {
      "enabled": false,
      "requests": 1000,
      "window": 900000
    },
    "cors": {
      "origins": ["https://gigrid.com", "https://www.gigrid.com"],
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "credentials": true
    },
    "waf": {
      "enabled": false
    }
  },
  "backup": {
    "enabled": false,
    "schedule": "weekly",
    "retention": 7
  }
}
